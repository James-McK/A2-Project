        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:A2_Project"
        mc:Ignorable="d"
        Title="CalanderTest" Width="1876" Height="1080" Closing="Window_Closing">
	<Grid x:Name="grd" Width="1876" Height="1080">
		<Label Name="lblCoOrds" Margin="0, 0, 0, 0" Content="Test" Height="30"/>
