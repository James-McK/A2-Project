		<Rectangle x:Name="rctRectOriginal2" Fill="LightSkyBlue" HorizontalAlignment="Left" Height="60" Stroke="Black" VerticalAlignment="Top" Width="100" MouseDown="Rectangle_MouseDown" MouseUp="RctRect_MouseUp" Margin="10,93,0,0"/>
	</Grid>
