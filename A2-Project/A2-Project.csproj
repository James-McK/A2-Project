<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>A2_Project</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\accounts.png" />
    <None Remove="Resources\addStaff.png" />
    <None Remove="Resources\calendar.png" />
    <None Remove="Resources\chart-box.png" />
    <None Remove="Resources\logowhite.png" />
    <None Remove="Resources\menubar.png" />
    <None Remove="Resources\notebook.png" />
    <None Remove="Resources\printer.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\accounts.png" />
    <Resource Include="Resources\addStaff.png" />
    <Resource Include="Resources\calendar.png" />
    <Resource Include="Resources\chart-box.png" />
    <Resource Include="Resources\logowhite.png" />
    <Resource Include="Resources\menubar.png" />
    <Resource Include="Resources\notebook.png" />
    <Resource Include="Resources\printer.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resource1.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource1.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resource1.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource1.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\emailPassword.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
